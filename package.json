{
    "name": "prettier-plugin-interpolated-html-tags",
    "version": "0.0.2",
    "homepage": "https://github.com/electrovir/prettier-plugin-interpolated-html-tags",
    "bugs": {
        "url": "https://github.com/electrovir/prettier-plugin-interpolated-html-tags/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/prettier-plugin-interpolated-html-tags"
    },
    "license": "MIT",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": "dist/index.js",
    "scripts": {
        "add-package": "npm run compile && rm -rf node_modules/prettier-plugin-interpolated-html-tags && cp -r dist node_modules/prettier-plugin-interpolated-html-tags",
        "compile": "rm -rf dist && virmator compile",
        "debug": "cross-env HTML_TAGS_DEBUG=true npm run test",
        "docs:update": "virmator code-in-markdown",
        "format": "npm run add-package && virmator format",
        "publish": "virmator publish \"npm run compile && npm run test:all\"",
        "test": "npm run add-package && virmator test",
        "test:all": "npm run test:types && npm run test:coverage && npm run test:spelling && npm run test:format && npm run test:docs",
        "test:coverage": "npm run test coverage",
        "test:docs": "virmator code-in-markdown check",
        "test:format": "npm run add-package && virmator format check",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit",
        "test:watch": "nodemon -V -w src -e ts --exec \"npm run test\""
    },
    "dependencies": {
        "@augment-vir/common": "^12.10.0",
        "@augment-vir/node-js": "^12.10.0"
    },
    "devDependencies": {
        "@augment-vir/chai": "^12.10.0",
        "@electrovir/nyc": "^15.1.0-fix0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.4",
        "@types/esprima": "^4.0.3",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.14.0",
        "@types/prettier": "^2.7.2",
        "chai": "^4.3.7",
        "cspell": "^6.26.3",
        "element-vir": "^9.2.0",
        "istanbul-smart-text-reporter": "^1.1.1",
        "markdown-code-example-inserter": "^0.3.0",
        "mocha": "^10.2.0",
        "mocha-spec-reporter-with-file-names": "^0.0.3",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.4",
        "prettier-plugin-jsdoc": "^0.4.2",
        "prettier-plugin-multiline-arrays": "^1.1.3",
        "prettier-plugin-organize-imports": "^3.2.2",
        "prettier-plugin-packagejson": "^2.4.3",
        "prettier-plugin-sort-json": "^1.0.0",
        "prettier-plugin-toml": "^0.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "virmator": "^5.4.1"
    }
}
